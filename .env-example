# LiveKit Configuration
LIVEKIT_API_KEY=""
LIVEKIT_API_SECRET=""
LIVEKIT_URL=""
NEXT_PUBLIC_LIVEKIT_URL=""

# Speech-to-Text Configuration
DEEPGRAM_API_KEY=""

# LLM Configuration
# Provider: "deepseek" (default), "openai", "vllm", "ollama", "lmstudio"
LLM_PROVIDER="deepseek"
# For DeepSeek (default): https://api.deepseek.com
# For OpenAI: leave LLM_BASE_URL empty or use https://api.openai.com/v1
# For vLLM: http://localhost:8001/v1
# For Ollama: http://localhost:11434/v1
# For LM Studio: http://localhost:1234/v1
LLM_BASE_URL="https://api.deepseek.com"
# Model name
# For DeepSeek: deepseek-chat (default)
# For OpenAI: gpt-4o-mini
# For vLLM: Qwen/Qwen2.5-7B-Instruct (or your deployed model)
# For Ollama: qwen2.5:7b-instruct (or your installed model)
# For LM Studio: (leave as configured in LM Studio)
LLM_MODEL="deepseek-chat"
# API Key (use "not-needed" for local providers, or your DeepSeek/OpenAI API key)
OPENAI_API_KEY=""

# TTS Configuration (KaniTTS)
# Base URL for KaniTTS server
KANI_BASE_URL="http://localhost:8000/v1"
# TTS Model - Spanish model for KaniTTS
TTS_MODEL="nineninesix/kani-tts-400m-es"
# Voice name
TTS_VOICE="andrew"