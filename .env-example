# LiveKit Configuration
LIVEKIT_API_KEY=""
LIVEKIT_API_SECRET=""
LIVEKIT_URL=""

# ============================================
# Speech-to-Text (STT) Configuration
# All OpenAI-compatible STT endpoints supported
# ============================================

# Option 1: Local Parakeet TDT (Recommended - Ultra-fast, local, 25+ languages)
PARAKEET_BASE_URL="http://localhost:5092/v1"
STT_MODEL="parakeet-tdt-0.6b-v3"

# Option 2: OpenAI Whisper (Cloud)
# PARAKEET_BASE_URL="https://api.openai.com/v1"
# STT_MODEL="whisper-1"

# Option 3: Any OpenAI-compatible STT provider
# PARAKEET_BASE_URL="https://your-stt-provider.com/v1"
# STT_MODEL="your-stt-model"

# ============================================
# Language Model (LLM) Configuration
# All OpenAI-compatible LLM endpoints supported
# ============================================

# Choose one LLM provider:

# Option 1: DeepSeek (Recommended - Cost-effective, high quality)
LLM_PROVIDER="deepseek"
LLM_BASE_URL="https://api.deepseek.com"
LLM_MODEL="deepseek-chat"
OPENAI_API_KEY=""

# Option 2: OpenAI (Cloud)
# LLM_PROVIDER="openai"
# LLM_BASE_URL="https://api.openai.com/v1"
# LLM_MODEL="gpt-4o-mini"
# OPENAI_API_KEY=""

# Option 3: Ollama (Local)
# LLM_PROVIDER="ollama"
# LLM_BASE_URL="http://localhost:11434/v1"
# LLM_MODEL="qwen2.5:7b-instruct"
# OPENAI_API_KEY="not-needed"

# Option 4: vLLM (Local, high performance)
# LLM_PROVIDER="vllm"
# LLM_BASE_URL="http://localhost:8001/v1"
# LLM_MODEL="Qwen/Qwen2.5-7B-Instruct"
# OPENAI_API_KEY="not-needed"

# Option 5: LM Studio (Local, user-friendly)
# LLM_PROVIDER="lmstudio"
# LLM_BASE_URL="http://localhost:1234/v1"
# LLM_MODEL="qwen2.5-7b-instruct"
# OPENAI_API_KEY="not-needed"

# Option 6: Any other OpenAI-compatible LLM provider
# LLM_PROVIDER="custom"
# LLM_BASE_URL="https://your-llm-provider.com/v1"
# LLM_MODEL="your-model-name"
# OPENAI_API_KEY=""

# ============================================
# Text-to-Speech (TTS) Configuration
# All OpenAI-compatible TTS endpoints supported
# ============================================

# Option 1: OpenAI TTS (Cloud - Recommended for quality)
# KANI_BASE_URL="https://api.openai.com/v1"
# TTS_MODEL="tts-1"
# TTS_VOICE="alloy"

# Option 2: Local KaniTTS (Spanish model)
KANI_BASE_URL="http://localhost:8000/v1"
TTS_MODEL="nineninesix/kani-tts-400m-es"
TTS_VOICE="andrew"

# Option 3: Any OpenAI-compatible TTS provider
# KANI_BASE_URL="https://your-tts-provider.com/v1"
# TTS_MODEL="your-tts-model"
# TTS_VOICE="your-voice-name"